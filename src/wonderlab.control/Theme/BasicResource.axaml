<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Add Resources Here -->
	<Color x:Key="AccentColor">Green</Color>
	<Color x:Key="AccentColorLight1">#00FFFFFF</Color>
	<Color x:Key="AccentColorLight2">#00FFFFFF</Color>
	<Color x:Key="AccentColorLight3">#00FFFFFF</Color>
	<Color x:Key="AccentColorDark1">#00FFFFFF</Color>
	<Color x:Key="AccentColorDark2">#00FFFFFF</Color>
	<Color x:Key="AccentColorDark3">#00FFFFFF</Color>

	<SolidColorBrush x:Key="AccentBrush" Color="{DynamicResource AccentColor}"/>
	<SolidColorBrush x:Key="AccentBrushLight1" Color="{DynamicResource AccentColorLight1}"/>
	<SolidColorBrush x:Key="AccentBrushLight2" Color="{DynamicResource AccentColorLight2}"/>
	<SolidColorBrush x:Key="AccentBrushLight3" Color="{DynamicResource AccentColorLight3}"/>
	<SolidColorBrush x:Key="AccentBrushDark1" Color="{DynamicResource AccentColorDark1}"/>
	<SolidColorBrush x:Key="AccentBrushDark2" Color="{DynamicResource AccentColorDark2}"/>
	<SolidColorBrush x:Key="AccentBrushDark3" Color="{DynamicResource AccentColorDark3}"/>
</ResourceDictionary>
