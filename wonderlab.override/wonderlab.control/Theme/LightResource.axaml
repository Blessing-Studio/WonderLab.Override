<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Dialog Color -->
	<SolidColorBrush x:Key="DialogBackground" Color="#4C000000"/>
	<SolidColorBrush x:Key="DialogContentBackground" Color="#fff3f3f3"/>
	<SolidColorBrush x:Key="DialogContentBorderBrush" Color="#64757575"/>

	<SolidColorBrush x:Key="ControlFillColorDefault" Color="#B3FFFFFF"/>
	
	<!--Button Color-->
	<SolidColorBrush x:Key="TransparentBackground" Color="#00FFFFFF"/>
	<SolidColorBrush x:Key="TransparentForeground" Color="White"/>
	<SolidColorBrush x:Key="TransparentBorderBrush" Color="Transparent"/>

	<BoxShadow x:Key="BottomActionBarBoxShadow">0 8 32 0 #4C000000</BoxShadow>
	<!--Font Color-->
	<SolidColorBrush x:Key="FontColorBrush" Color="Black"/>
</ResourceDictionary>
