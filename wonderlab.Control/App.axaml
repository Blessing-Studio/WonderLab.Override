<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="wonderlab.control.App">
    <Application.Styles>
        <FluentTheme Mode="Light"/>
		<StyleInclude Source="/Style/Dialog/MessageDialogStyle.axaml"/>
		<StyleInclude Source="/Style/Button/AccentButtonStyle.axaml"/>
		<StyleInclude Source="/Style/Bar/ToolBarStyle.axaml"/>
		<StyleInclude Source="/Style/Dialog/UpdateDialogStyle.axaml"/>
		<StyleInclude Source="/Style/Bar/MessageTipsBarStyle.axaml"/>
		<StyleInclude Source="/Style/Bar/BottomActionBarStyle.axaml"/>
		<StyleInclude Source="/Style/Bar/ProgressBar.axaml"/>
		<StyleInclude Source="/Style/FilpViewStyle.axaml"/>
		<StyleInclude Source="/Style/Dialog/AccountDialogStyle.axaml"/>
		<StyleInclude Source="/Style/Dialog/AccountTypeDialogStyle.axaml"/>
    </Application.Styles>
	
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="avares://wonderlab.Control/Theme/BasicResource.axaml"/>
				<ResourceInclude Source="avares://wonderlab.Control/IconPack.axaml"/>
				<ResourceInclude Source="avares://wonderlab.Control/Theme/LightResource.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
