<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Dialog Color -->
	<SolidColorBrush x:Key="DialogBackground" Color="#4C000000"/>
	<SolidColorBrush x:Key="DialogContentBackground" Color="#fff3f3f3"/>
	<SolidColorBrush x:Key="DialogContentBorderBrush" Color="#64757575"/>

	<SolidColorBrush x:Key="ControlFillColorDefault" Color="#B3FFFFFF"/>
	
	<!--Button Color-->
	<SolidColorBrush x:Key="TransparentBackground" Color="#00FFFFFF"/>
	<SolidColorBrush x:Key="TransparentForeground" Color="White"/>
	<SolidColorBrush x:Key="TransparentBorderBrush" Color="Transparent"/>
	<SolidColorBrush x:Key="NormalButtonBackground" Color="#B3FFFFFF"/>
	<!-- Elevation border brushes (See note above for hack to get same effect)-->
	<LinearGradientBrush x:Key="NormalButtonBorderBrush" StartPoint="0,0" EndPoint="0,3">
		<LinearGradientBrush.GradientStops>
			<GradientStop Offset="1" Color="#29000000" />
			<GradientStop Offset="0.33" Color="#0F000000" />
		</LinearGradientBrush.GradientStops>
	</LinearGradientBrush>
	<!--Font Color-->
	<SolidColorBrush x:Key="FontColorBrush" Color="Black"/>
</ResourceDictionary>
