<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!-- Color -->
	<Color x:Key="DividerColor">#11111126</Color>
	<Color x:Key="DefaultColor400">#a1a1aa</Color>
	
	<!--Layout-->
	<SolidColorBrush x:Key="FocusBrush" Color="#006FEE"/>
	<SolidColorBrush x:Key="LayoutBackground" Color="#FFFFFF"/>
	<SolidColorBrush x:Key="LayoutForeground" Color="#11181C"/>
	<SolidColorBrush x:Key="DividerBrush" Color="{StaticResource DividerColor}"/>
	
	<!--Content-->
